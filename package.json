{
  "name": "mcpr",
  "version": "0.0.1",
  "description": "The ultimate Minecraft plugin registry. ",
  "main": "server.js",
  "scripts": {
    "test": "gulp lint --fail && snyk test",
    "postinstall": "bower install --allow-root && gulp build",
    "start": "node server/server.js",
    "dev": "gulp serve",
    "lint": "gulp lint",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "docs": "mkdocs build --clean && apidoc -i server/api/ -o docs-site/api/endpoints"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcpr/mcpr.git"
  },
  "keywords": [
    "minecraft",
    "plugins",
    "registry",
    "mods",
    "mcpr"
