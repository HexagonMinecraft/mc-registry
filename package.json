  "author": "Filiosoft, LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcpr/mcpr/issues"
  },
  "homepage": "https://registry.HexagonMinecraft.com",
  "dependencies": {
    "aws-sdk": "^2.100.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
