    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "global": "^4.3.2",
    "handlebars": "^4.0.10",
    "morgan": "^1.8.2",
    "nano": "^6.3.0",
    "showdown": "^1.7.1",
    "snyk": "^1.33.1"
  },
  "devDependencies": {
    "bower": "1.8.0",
