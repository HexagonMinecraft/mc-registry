{
  "name": "mcpr",
  "version": "0.0.1",
  "description": "The ultimate Minecraft plugin registry.",
  "main": "server.js",
  "scripts": {
    "test": "npm run lint -- --fail && npm audit || true",
    "build": "echo No build...",
    "postinstall": "npm run build",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint:report": "eslint . -f html -o tests/eslint-report.html || true && eslint . -f junit -o tests/eslint-report.xml || true",
    "lint": "npm run lint:report && eslint .",
    "lint:fix": "eslint . --fix",
    "docs": "mkdocs build --clean && apidoc -i api/ -o docs-site/api/endpoints"
  },
  "repository": "mcpr/mcpr",
  "keywords": [
    "minecraft",
    "plugins",
    "registry",
    "mods",
    "mcpr"
  ],
  "author": "Noah Prail <noah@prail.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcpr/mcpr/issues"
  },
  "homepage": "https://github.com/mcpr/mcpr",
  "dependencies": {
    "aws-sdk": "^2.651.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.1",
    "express-jwt": "^5.3.1",
    "express-status-monitor": "^1.2.10",
    "helmet": "^3.22.0",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.6",
    "semver": "^7.1.3",
    "serve-favicon": "^2.4.5",
    "slugify": "^1.4.0",
    "uuid": "^7.0.3",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "apidoc": "^0.20.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2"
  },
  "apidoc": {
    "title": "MCPR API Docs",
    "url": "https://mcpr.io/api/v1",
    "sampleUrl": "https://mcpr.io/api/v1",
    "template": {
      "withCompare": false
    }
  }
}
